{
	"$schema": "https://turbo.build/schema.json",
	"globalDependencies": ["**/.env.*local"],
	"pipeline": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**"]
		},
		"build:dev": {
			"dependsOn": ["^build:dev"],
			"outputs": ["dist/**"]
		},
		"build:prod": {
			"dependsOn": ["^build:prod"],
			"outputs": ["dist/**"]
		},
		"start": {
			"cache": false,
			"persistent": true
		},
		"lint": {
			"outputs": []
		},
		"lint:js": {
			"outputs": []
		},
		"lint:css": {
			"outputs": []
		},
		"lint:php": {
			"outputs": []
		},
		"format": {
			"outputs": []
		},
		"format:js": {
			"outputs": []
		},
		"format:css": {
			"outputs": []
		},
		"format:php": {
			"outputs": []
		},
		"clean": {
			"cache": false
		}
	}
}
