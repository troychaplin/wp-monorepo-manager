{
	"extends": [
		"plugin:@wordpress/eslint-plugin/recommended"
	],
	"env": {
		"browser": true,
		"es6": true
	},
	"parserOptions": {
		"requireConfigFile": false,
		"ecmaVersion": 2021,
		"sourceType": "module"
	},
	"rules": {
		"import/no-extraneous-dependencies": "off",
		"camelcase": "off"
	}
}